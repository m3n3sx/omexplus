# Admin Dashboard Environment Variables

# API URL (Medusa backend)
NEXT_PUBLIC_API_URL=https://ooxo.pl
NEXT_PUBLIC_STOREFRONT_URL=https://ooxo.pl

# Gemini AI API (for SEO content generation)
GEMINI_API_KEY=your_gemini_api_key

# Google Analytics Data API
# Get from Google Cloud Console -> APIs & Services -> Credentials
GA_PROPERTY_ID=458820636
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Google Ads API
# Get from Google Ads -> Tools & Settings -> API Center
GOOGLE_ADS_CUSTOMER_ID=751-186-138
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token
GOOGLE_ADS_CLIENT_ID=your_client_id.apps.googleusercontent.com
GOOGLE_ADS_CLIENT_SECRET=your_client_secret
GOOGLE_ADS_REFRESH_TOKEN=your_refresh_token

# Set to 'true' when Google APIs are configured
NEXT_PUBLIC_GOOGLE_CONFIGURED=false
